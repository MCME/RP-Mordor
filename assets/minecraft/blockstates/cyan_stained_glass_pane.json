{
  "multipart": [
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "block/glass_pane_half_right_cyan"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "block/glass_pane_half_right_cyan",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "block/glass_pane_half_left_cyan"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "block/glass_pane_half_left_cyan",
        "y": 90
      }
    },
    {
      "when": {
        "north": false,
        "east": false,
        "south": false,
        "west": false
      },
      "apply": {
        "model": "block/unused_cross_0"
      }
    }
  ]
}
